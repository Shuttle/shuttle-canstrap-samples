<h1>cs-button</h1>
<h3>default</h3>
<cs-button text:from="'some button'" click:from="click" elementClass:from="'btn-primary'" />
<cs-button text:from="'icon button'" click:from="click" iconNameClass:from="'fa-play-circle-o'" elementClass:from="'btn-secondary'" />
<h3>actions</h3>
<cs-button actions:from="actions" text:from="'action'" />
